{
  "Repository": "cli",
  "Definitions": {
    "Path": ".",
    "Type": "VSTS",
    "BaseUrl": "https://devdiv.visualstudio.com/DefaultCollection"
  },
  "Pipelines": [
    {
      "Name": "Trusted-All-Release",
      "Parameters": {
        "TreatWarningsAsErrors": "false"
      },
